<?xml version="1.0" encoding="utf-8" ?> 
<control xmlns:def="Definition" xmlns="http://schemas.sitecore.net/Visual-Studio-Intellisense">
  <CommandPortletCommand def:renderas="Literal">           
    <Border Padding="2px 8px 2px 0px" Style="display:inline" Width="200">
      <Inline Class="scCommand" ID="$ID" Click="$Click" RollOver="true" ToolTip="$ToolTip" TabStop="true">
        <ThemedImage Src="$Icon" Width="24" Height="24" Margin="0px 6px 0px 6px" Align="absmiddle"/>
        <Literal Class="scClickFont" Text="$Header"/>
      </Inline>
    </Border>
  </CommandPortletCommand>
</control>
