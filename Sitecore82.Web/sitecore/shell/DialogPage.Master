  </asp:ContentPlaceHolder>
  <link href="/sitecore/shell/Themes/Standard/Default/Dialogs.css" rel="stylesheet" />
</head>
<body>
  <form runat="server">
    <div class="scFlexColumnContainer" style="height: 100%;">

      <asp:ContentPlaceHolder ID="DialogHeader" runat="server">
      </asp:ContentPlaceHolder>
      <div class="scFlexContent scFormDialogContent">
