<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="DialogPage.master.cs" ViewStateMode="Disabled" EnableViewState="false" Inherits="Sitecore.Shell.MasterDialog" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
