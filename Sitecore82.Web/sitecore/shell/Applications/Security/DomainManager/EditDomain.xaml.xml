<?xml version="1.0" encoding="UTF-8" ?>
<xamlControls 
  xmlns:x="http://www.sitecore.net/xaml"
  xmlns:ajax="http://www.sitecore.net/ajax"
  xmlns:rest="http://www.sitecore.net/rest"
  xmlns:r="http://www.sitecore.net/renderings"
  xmlns:xmlcontrol="http://www.sitecore.net/xmlcontrols"
  xmlns:p="http://schemas.sitecore.net/Visual-Studio-Intellisense"
  xmlns:asp="http://www.sitecore.net/microsoft/webcontrols"
  xmlns:html="http://www.sitecore.net/microsoft/htmlcontrols"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
  
  <Sitecore.Shell.Applications.Security.DomainManager.EditDomain x:inherits="Sitecore.Shell.Applications.Security.DomainManager.EditDomainPage,Sitecore.Client">
    <Sitecore.Controls.DialogPage Header="Edit Domain" Icon="People/32x32/users4.png" Text="Edit a domain." runat="server">
      <AjaxScriptManager runat="server"/>
      <ContinuationManager runat="server" />

      <div style="padding:4px 0px 0px 0px">
        <asp:CheckBox ID="LocallyManagedDomain" runat="server" />
        <asp:Label runat="server" AssociatedControlID="LocallyManagedDomain">
          <Literal Text="Locally Managed Domain" runat="server"/>
        </asp:Label>
      </div>

    </Sitecore.Controls.DialogPage>
  </Sitecore.Shell.Applications.Security.DomainManager.EditDomain>

</xamlControls>
