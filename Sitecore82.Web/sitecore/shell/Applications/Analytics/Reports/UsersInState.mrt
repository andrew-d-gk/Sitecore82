  <Script><![CDATA[using System;
using Sitecore.Analytics.Model.Entities;
using Sitecore.Security.Accounts;
using System.Drawing;
using System.Windows.Forms;
using System.Data;
using Stimulsoft.Controls;
using Stimulsoft.Base.Drawing;
using Stimulsoft.Report;
using Stimulsoft.Report.Dialogs;
using Stimulsoft.Report.Components;
using System.Web.Profile;

namespace Reports
{
    
    public class LatestSessionsReport : Sitecore.Analytics.Reports.ReportBase
    {
        
        public LatestSessionsReport()
        {
            this.InitializeComponent();
			
        }
        #region StiReport Designer generated code - do not modify
		#endregion StiReport Designer generated code - do not modify
		
		public string GetEmail(Guid contactid)
		{
			return this.GetEmailFromContactFacets(contactid) ?? this.GetEmailFromUser(contactid) ?? "-";
		}
		
		public string GetFullName(Guid contactid)
		{
			if(contactid == Guid.Empty) return "Anonymous user";
			var info = this.GetContactFacet<IContactPersonalInfo>(contactid);
			if(info == null)
			{
				var contact = this.GetContact(contactid);
				var identifier = contact != null ? contact.Identifiers.Identifier : null;
				return identifier ?? "Anonymous user";
			}
    
			if(string.IsNullOrEmpty(info.FirstName)) 
				return info.Surname ?? this.GetContact(contactid).Identifiers.Identifier ?? "Anonymous user";
			var surname = string.IsNullOrEmpty(info.Surname) ? string.Empty : " " + info.Surname;
			return info.FirstName + surname;
		}

		protected string GetVisitCount(Guid contactid)
		{
			if(contactid == Guid.Empty) return "-";
			var contact = this.GetContact(contactid);
			var count = contact.System.VisitCount;
			return count == 0 ? "-" : count.ToString();
		}
   
		protected T GetValue<T>(Guid contactid, Func<int, T> format)
		{
			if(contactid == Guid.Empty) return format(0);
			var contact = this.GetContact(contactid);
			return format(contact.System.Value);
		}
    
    private string GetEmailFromContactFacets(Guid contactid)
    {
			var emails = this.GetContactFacet<IContactEmailAddresses>(contactid);
			if(emails == null) return null;
			var preferred = emails.Preferred;
			if(preferred == null) return null;
			if(!emails.Entries.Contains(preferred)) return null;
			return emails.Entries[preferred].SmtpAddress ?? null;
    }

    private string GetEmailFromUser(Guid contactid)
    {
			if(contactid == Guid.Empty) return null;
			var contact = this.GetContact(contactid);
			if(contact == null) return null;
			var username = contact.Identifiers.Identifier;
			if(username == null) return null;
			var user = User.FromName(username, false);
			if(user == null) return null;
			return user.Profile.Email;
    }

		private T GetContactFacet<T>(Guid contactid) where T : class
		{
			if(contactid == Guid.Empty) return null;
			var contact = this.GetContact(contactid);
			if(contact == null) return null;
			foreach(var facet in contact.Facets)
			{
				var typedFacet = facet.Value as T;
				if(typedFacet == null) continue;
				return typedFacet;
			}
			return null;
		}
		
		public static string GetStateName(object stateid)
		{
			Sitecore.Data.Items.Item stateItem = Sitecore.Context.ContentDatabase.GetItem(stateid.ToString());
			return stateItem == null ? "-" : string.Format("{0} of {1}", stateItem.DisplayName, stateItem.Parent.DisplayName);
		
		}
    
    public static int GetDaysInState(DateTime entry)
		{
			var now = Sitecore.Common.DateTimeProvider.GetUtcNow();
      return now.Subtract(entry).Days;
		}
	
    }
}
